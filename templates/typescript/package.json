{
  "name": "mf_application",
  "version": "1.0.0",
  "description": "Base micro-frontend application",
  "main": "index.js",
  "keywords": [
    "Webpack",
    "Module",
    "Federation",
    "javascript",
    "typescript",
  ],
  "config": {
    "analyze": false
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config ./webpack/webpack.development.js",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.production.js",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx --no-error-on-unmatched-pattern src/",
    "lint:fix": "eslint --ext .ts,.tsx,.js,.jsx --no-error-on-unmatched-pattern --quiet --fix",
    "pretest": "yarn run test:clean && yarn run lint",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test jest --config jest.config.js --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "prettify": "prettier . --write",
    "check-types": "tsc"
  },
  "dependencies": {
    "clean-webpack-plugin": "3.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "typescript": "3.9.7",
    "webpack": "5.0.0-beta.25",
    "webpack-merge": "5.1.2"
  },
  "devDependencies": {
    "@babel/core": "7.11.4",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/plugin-proposal-export-namespace-from": "7.10.4",
    "@babel/plugin-proposal-function-sent": "7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "7.11.0",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-syntax-import-meta": "7.10.4",
    "@babel/plugin-transform-modules-commonjs": "7.10.4",
    "@babel/preset-env": "7.11.0",
    "@babel/preset-react": "7.10.4",
    "@babel/preset-typescript": "7.10.4",
    "@types/enzyme": "3.10.5",
    "@types/jest": "26.0.10",
    "@types/node": "14.6.0",
    "@types/react": "16.9.46",
    "@types/react-dom": "16.9.8",
    "@typescript-eslint/eslint-plugin": "3.10.0",
    "@typescript-eslint/parser": "3.10.0",
    "babel-jest": "26.3.0",
    "babel-loader": "8.1.0",
    "babel-plugin-dynamic-import-node": "2.3.3",
    "babel-plugin-named-asset-import": "0.3.6",
    "babel-preset-react-app": "9.1.2",
    "circular-dependency-plugin": "5.2.0",
    "compression-webpack-plugin": "5.0.1",
    "cross-env": "7.0.2",
    "css-loader": "^4.2.2",
    "dotenv": "^8.2.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.3",
    "eslint": "7.7.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-node": "4.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-react": "1.1.7",
    "eslint-import-resolver-webpack": "0.12.2",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-package-json": "0.1.4",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.6",
		"eslint-plugin-react-hooks": "4.1.0",
		"file-loader": "6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.4.2",
    "jest-css-modules": "2.1.0",
    "jest-enzyme": "7.1.2",
    "jest-watch-typeahead": "0.6.0",
    "lint-staged": "10.2.11",
    "mini-css-extract-plugin": "^0.10.0",
    "node-sass": "4.14.1",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "prettier": "2.1.0",
    "raf": "3.4.1",
    "react-hot-loader": "4.12.21",
    "react-scripts": "3.4.3",
    "react-test-renderer": "16.13.1",
    "sass-loader": "^10.0.0",
    "style-loader": "1.2.1",
    "stylelint": "13.6.1",
    "stylelint-config-sass-guidelines": "7.0.0",
    "terser-webpack-plugin": "4.1.0",
    "ts-jest": "26.2.0",
		"typescript-plugin-css-modules": "2.4.0",
		"url-loader": "4.1.0",
    "webpack-bundle-analyzer": "3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "yarn run lint:fix",
      "git add --force"
    ],
    "*.{md,json}": [
      "prettier --write",
      "git add --force"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": ""
  },
