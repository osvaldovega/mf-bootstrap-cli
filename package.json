{
	"name": "@mfbootstrap/create-project",
	"version": "1.0.0",
	"description": "Bootstrap to create projects with Webpack Module Federation Plugin",
	"main": "src/index.js",
	"private": true,
	"bin": {
		"@mfbootstrap/create-project": "bin/create-project",
		"mf-bootstrap": "bin/create-project"
	},
	"publishConfig": {
		"access": "public"
	},
	"type": "commonjs",
	"scripts": {
		"lint": "eslint src/"
	},
	"keywords": [
		"webpack",
		"module",
		"federation",
		"javascript",
		"bootstrap",
		"cli",
		"create-project",
		"react"
	],
	"dependencies": {
		"arg": "4.1.3",
		"esm": "3.2.25",
		"execa": "4.0.2",
		"inquirer": "7.3.0",
		"listr": "0.14.3",
		"ncp": "2.0.0",
		"pkg-install": "1.0.0"
	},
	"devDependencies": {
		"chalk": "4.1.0",
		"chalk-pipe": "4.0.0",
		"eslint": "7.4.0",
		"eslint-config-prettier": "6.11.0",
		"eslint-plugin-standard": "4.0.1",
		"husky": "4.2.5",
		"validate-npm-package-name": "3.0.0"
	},
	"author": "Osvaldo Vega A (OvEgA)",
	"repository": {
		"type": "git",
		"url": "https://github.com/osvaldovega/mf-bootstrap.git"
	},
	"engines": {
		"node": ">=10"
	},
	"license": "ISC",
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,md,css,html}": [
			"prettier --trailing-comma es5 --single-quote --write"
		]
	}
}
