{
	"name": "@mfbootstrap/create-project",
	"version": "1.0.0",
	"description": "Bootstrap to create projects with Webpack Module Federation Plugin",
	"main": "src/index.js",
	"bin": {
		"@mfbootstrap/create-project": "bin/create-project",
		"create-project": "bin/create-project"
	},
	"publishConfig": {
		"access": "public"
	},
	"type": "commonjs",
	"scripts": {
		"lint": "eslint src/"
	},
	"keywords": [
		"Webpack",
		"Module",
		"Federation",
		"javascript",
		"bootstrap",
		"cli",
		"create-project"
