	"author": "Osvaldo Vega A (OvEgA)",
	"license": "ISC",
	"dependencies": {
		"arg": "4.1.3",
		"chalk": "4.1.0",
		"esm": "3.2.25",
		"execa": "4.0.2",
		"inquirer": "7.3.0",
		"listr": "0.14.3",
		"ncp": "2.0.0",
		"pkg-install": "1.0.0"
	},
	"devDependencies": {
		"eslint": "7.4.0",
		"eslint-config-prettier": "6.11.0",
		"eslint-plugin-standard": "4.0.1",
		"husky": "4.2.5"
	},
	"engines": {
		"node": ">=8.10.0"
	}
